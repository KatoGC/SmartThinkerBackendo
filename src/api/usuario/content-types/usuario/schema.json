{
  "kind": "collectionType",
  "collectionName": "usuarios",
  "info": {
    "singularName": "usuario",
    "pluralName": "usuarios",
    "displayName": "usuario",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "lastName": {
      "type": "string"
    },
    "age": {
      "type": "integer"
    },
    "email": {
      "type": "email",
      "unique": true,
      "required": true
    },
    "password": {
      "type": "password",
      "required": false
    },
    "description": {
      "type": "text"
    },
    "role": {
      "type": "enumeration",
      "enum": [
        "Estudiante",
        "Profesor",
        "Administrador"
      ]
    },
    "occupation": {
      "type": "enumeration",
      "enum": [
        "Ingeniero de Software",
        "Médico",
        "Profesor",
        "Estudiante",
        "Abogado",
        "Diseñador Gráfico",
        "Contador",
        "Administrador de Empresas",
        "Enfermero",
        "Psicólogo",
        "Arquitecto",
        "Científico",
        "Escritor",
        "Periodista",
        "Músico",
        "Artista",
        "Consultor",
        "Gerente de Proyecto",
        "Analista de Datos",
        "Desarrollador Web",
        "Marketing",
        "Recursos Humanos",
        "Ventas",
        "Chef",
        "Electricista",
        "Fontanero",
        "Carpintero",
        "Piloto",
        "Mecánico",
        "Veterinario"
      ]
    },
    "activityCreator": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::actividad.actividad",
      "inversedBy": "userCreator"
    },
    "commentUser": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comentario.comentario",
      "mappedBy": "userComment"
    },
    "courseInstructor": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::curso.curso",
      "mappedBy": "userIntructor"
    }
  }
}
